[DEFAULT]
ignoreip = 127.0.0.1/8 ::1
bantime  = 300
findtime = 600
maxretry = 10

[sshd]
enabled  = true
port     = ssh
logpath  = /var/log/auth.log
maxretry = 10
findtime = 600
bantime  = 300

[mailcow]
enabled  = true
port     = 25,465,587,993,995
logpath  = /opt/mailcow/logs/mail.log
maxretry = 10
findtime = 600
bantime  = 300
